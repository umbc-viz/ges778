@article{S.S.R+2022,
  title = {Highlighting {{COVID-19}} Racial Disparities Can Reduce Support for Safety Precautions among {{White U}}.{{S}}. Residents},
  author = {{Skinner-Dorkenoo}, Allison L. and Sarmal, Apoorva and Rogbeer, Kasheena G. and Andr{\'e}, Chloe J. and Patel, Bhumi and Cha, Leah},
  year = {2022},
